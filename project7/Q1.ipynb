    "u = np.array([1, 2, 3])\n",
    "sigma = np.array([[3, -1, 1], [-1, 5, 3], [1, 3, 4]])\n",
    "\n",
    "A = np.linalg.cholesky(sigma)\n",
    "for i in range(20):\n",
    "    Z = np.array(np.random.rand(3))\n",
    "    X = np.dot(A, Z.T) + u\n",
    "    print('The '+ str(i+1)+' time: ', X)"
